{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "newline-after-var": ["error", "never"],
        "no-multiple-empty-lines": ["error", { "max": 0}],
        "object-curly-newline": ["error", "always"],
        "import/newline-after-import": "off"
    }
}
